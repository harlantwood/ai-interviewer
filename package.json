{
  "name": "svelte-kit-suprabase",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "rm -rf .svelte-kit && yarn sync && vite build",
    "build:cloudflare": "rm -rf .env .svelte-kit/ && yarn sync && export SVELTEKIT_ADAPTER=cloudflare && vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "nodemon --watch .env --watch package.json --watch svelte.config.js --watch tsconfig.json --watch yarn.lock --exec 'yarn dev:metal'",
    "dev:hammer": "nodemon --watch . --watch .env --ext ts,svelte,json,js --ignore .svelte-kit --exec 'yarn dev:metal'",
    "dev:metal": "clear && reset && export PORT=${PORT:-8888} && vite dev --port ${PORT} --host --clearScreen",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "yarn format && eslint --fix .",
    "preview": "vite preview",
    "sort": "yarn sort-package-json",
    "sync": "svelte-kit sync",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.1",
    "buffer": "^6.0.3",
    "openai": "^4.24.1"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sort-package-json": "^2.6.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "yarn": "^1.22.19"
  },
  "engineStrict": true
}
